az account set --subscription "mifurmsub"

# Create Resource Group
az group create -n tamopstfstates -l eastus2
 
# Create Storage Account
az storage account create -n mftamopstf -g tamopstfstates -l eastus2 --sku Standard_LRS
 
# Create Storage Account Container
az storage container create -n tfstatedevops --account-name mftamopstf

# Create Service Principal 
az ad sp create-for-rbac --name tamopstf2
